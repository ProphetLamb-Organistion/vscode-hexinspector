{
	"name": "hexinspector",
	"displayName": "HexInspector",
	"description": "Show conversions of the pointed hexadecimal value",
	"version": "0.0.1",
	"publisher": "mateuszchudyk",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/mateuszchudyk/vscode-hexinspector"
	},
	"keywords": [
		"hex",
		"inspector",
		"hover",
		"converter"
	],
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"languages": [{
			"id": "hexinspector"
		}],
		"grammars": [
			{
				"language": "hexinspector",
				"scopeName": "source.hexinspector",
				"path": "./syntaxes/hexinspector.json"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
