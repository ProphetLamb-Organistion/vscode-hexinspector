{ 
	"scopeName": "source.hexinspector",
	"fileTypes": [ "hexinspector" ],
	"patterns": [
		{
			"match": "HexInspector:",
			"name": "markup.heading.2.html"
		},
		{
			"match": "(Little Endian|Big Endian)",
			"name": "entity.name.function"
		},
		{
			"match": "0x([0-9a-fA-F]+)",
			"name": "entity.name.class"
		},
		{
			"match": "[0-9]+B",
			"name": "entity.name.function"
		},
		{
			"match": "(Decimal|Binary|Float16|Float32|Float64|Chars|Size)",
			"name": "entity.name.type"
		}
	]
}